	    [passed=no; AC_MSG_RESULT([gr-qtgui requires libQtGui >= 4..])])
        PKG_CHECK_MODULES(QTOPENGL, QtOpenGL >= 4.4, [],
	    [passed=no; AC_MSG_RESULT([gr-qtgui requires libQtOpenGL >- 4.4.])])
	
